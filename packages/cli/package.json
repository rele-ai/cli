{
  "name": "@releai/cli",
  "description": "RELE.AI CLI Tool",
  "version": "0.1.17",
  "author": "Elon Salfati @ElonSalfati",
  "bin": {
    "rb": "bin/run"
  },
  "bugs": "https://github.com/rele-ai/cli/issues",
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "chai": "^4.3.4",
    "eslint": "^7.27.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^11.0.3",
    "mocha": "^8.4.0",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src",
    "/web",
    "/scripts",
    "/files",
    "/lib"
  ],
  "homepage": "https://github.com/rele-ai/cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache License",
  "main": "src/index.js",
  "oclif": {
    "commands": "./src/commands",
    "bin": "rb",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "rele-ai/cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint .",
    "postinstall": "node ./scripts/copy-configs.js",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.6.2",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@releai/default-groups": "^0.1.17",
    "axios": "^0.21.1",
    "cli-ux": "^5.5.1",
    "consolidate": "^0.16.0",
    "csv": "^5.5.0",
    "download-git-repo": "^3.0.2",
    "express": "^4.17.1",
    "glob": "^7.1.7",
    "google-protobuf": "^3.17.1",
    "grpc": "^1.24.5",
    "handlebars": "^4.7.7",
    "inquirer": "^8.1.0",
    "jjv": "^1.0.2",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^3.1.2",
    "metalsmith": "^2.3.0",
    "mustache-express": "^1.3.0",
    "ngrok": "^4.0.1",
    "nodemon": "^2.0.7",
    "npm": "^7.14.0",
    "npm-check-updates": "11.5.13",
    "open": "^8.2.0",
    "pkg-dir": "^5.0.0",
    "rimraf": "^3.0.2",
    "simple-git": "^2.39.0"
  }
}
